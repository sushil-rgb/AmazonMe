{
    "actorSpecification": 1,
    "name": "Amazon scraper",
    "title": "Amazon-scraper",
    "version": "1.0.0",
    "meta": {
        "templateId": "python-start"
    },
    "input": "./input_schema.json",
    "dockerfile": "/Dockerfile",
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "views": {
                "overview": {
                    "title": "Overview",
                    "transformation": {
                        "fields": [
                            "numericField",
                            "nameField",
                            "descriptionField",
                            "priceField",
                            "dealPriceField",
                            "savedPriceField",
                            "ratingField",
                            "ratingCountField",
                            "availabilityField",
                            "storeField",
                            "storeLinkField",
                            "pictureUrlField",
                            "picturesUrlField",
                            "linkUrlField"
                        ]
                    },
                    "display": {
                        "component": "table",
                        "properties": {
                            "numericField": {
                                "label": "ID",
                                "format": "number"
                            },
                            "nameField": {
                                "label": "Name",
                                "format": "text"
                            },
                            "descriptionField": {
                                "label": "Description",
                                "format": "text"
                            },
                            "priceField": {
                                "label": "Price",
                                "format": "text"
                            },
                            "dealPriceField": {
                                "label": "Deal Price",
                                "format": "text"
                            },
                            "savedPriceField": {
                                "label": "Saved Price",
                                "format": "text"
                            },
                            "ratingField": {
                                "label": "Rating",
                                "format": "text"
                            },
                            "ratingCountField": {
                                "label": "Rating Count",
                                "format": "text"
                            },
                            "availabilityField": {
                                "label": "Availability",
                                "format": "text"
                            },
                            "storeField": {
                                "label": "Store",
                                "format": "text"
                            },
                            "storeLinkField": {
                                "label": "Store Link",
                                "format": "link"
                            },
                            "pictureUrlField": {
                                "label": "Picture URL",
                                "format": "link"
                            },
                            "picturesUrlField": {
                                "label": "Pictures URL",
                                "format": "list"
                            },
                            "linkUrlField": {
                                "label": "Link URL",
                                "format": "link"
                            }
                        }
                    }
                }
            }
        }
    }
}
